{
  "name": "@gprelatto/leovegas",
  "version": "0.0.1",
  "description": "",
  "author": "Gonzalo Prelatto",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npx mkdirp dist/src/collection",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "NODE_ENV=dev nest start --debug --watch",
    "start:prod-local": "NODE_ENV=prod nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d datasource.config.ts",
    "generate": "npm run typeorm migration:generate",
    "migrate": "npm run typeorm migration:run",
    "rollback": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@algoan/nestjs-http-exception-filter": "^1.0.20",
    "@algoan/nestjs-logging-interceptor": "^2.3.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.0.8",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.2.3",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.1.4",
    "axios": "^1.6.8",
    "base-path-converter": "^1.0.2",
    "bcrypt": "^5.1.1",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "config": "^3.3.9",
    "express": "^4.18.1",
    "fastify-swagger": "^5.2.0",
    "got": "^12.1.0",
    "handlebars": "^4.7.7",
    "install": "^0.13.0",
    "node-ssh": "^13.2.0",
    "nodemailer": "^6.7.0",
    "npm": "^11.1.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "recursive-fs": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.4.7",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.2",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.4",
    "ts-jest": "^28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "<4.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
